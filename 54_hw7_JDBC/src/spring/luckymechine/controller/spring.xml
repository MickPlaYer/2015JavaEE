<?xml version="1.0" encoding="UTF-8"?> 
<beans xmlns="http://www.springframework.org/schema/beans"
	xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
	xsi:schemaLocation="http://www.springframework.org/schema/beans
	http://www.springframework.org/schema/beans/spring-beans-4.1.xsd">

	<bean id="ENTRY_PAGE" class="java.lang.String">
		<constructor-arg type="String" value="spring/luckymechine/entry"/>
    </bean>
    
    <bean id="MAIN_PAGE" class="java.lang.String">
		<constructor-arg type="String" value="spring/luckymechine/luckymechine"/>
    </bean>
	
	<bean id="RICH_RANK_PAGE" class="java.lang.String">
		<constructor-arg type="String" value="spring/luckymechine/richrank"/>
    </bean>

    <bean id="ERROR_PAGE" class="java.lang.String">
		<constructor-arg type="String" value="spring/database/error"/>
    </bean>
</beans>